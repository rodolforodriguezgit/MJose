{
  "name": "sales-data-lake",
  "version": "1.0.0",
  "description": "Sistema de consulta y an√°lisis de ventas desde Data Lake",
  "private": true,
  "scripts": {
    "dev": "node scripts/dev.js",
    "dev:back": "cd back && ts-node-dev --respawn --transpile-only src/index.ts",
    "dev:front": "cd front && vite",
    "build": "npm run build:back && npm run build:front",
    "build:back": "cd back && tsc",
    "build:front": "cd front && tsc && vite build",
    "start": "npm run start:back & npm run start:front",
    "start:back": "cd back && node dist/index.js",
    "start:front": "cd front && vite preview",
    "lint": "npm run lint:back && npm run lint:front",
    "lint:back": "cd back && eslint src/**/*.ts",
    "lint:front": "cd front && eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
  },
  "keywords": [
    "sales",
    "data-lake",
    "api",
    "react",
    "typescript",
    "clean-architecture"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.10.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.8"
  }
}
